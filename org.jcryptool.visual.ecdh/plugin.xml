<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.jcryptool.visual"
            class="org.jcryptool.visual.ECDH.ui.view.ECDHView"
            icon="icons/view.gif"
            id="org.jcryptool.visual.ECDH.view"
            name="%view.name.0"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/toc.xml"
            primary="false"/>
   </extension>
   <extension point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.jcryptool.core.operations.visuals">
      <Visual
            name="%Visual.name.0"
            viewId="org.jcryptool.visual.ECDH.view"
            contextHelpId="org.jcryptool.visual.ecdh.ecdhview">
      </Visual>
   </extension>

</plugin>
