<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            name="%extViewName"
            icon="icons/view.gif"
            category="org.jcryptool.analysis"
            class="org.jcryptool.analysis.friedman.views.FriedmanView"
            id="org.jcryptool.analysis.friedman.views.FriedmanView">
      </view>
   </extension>
   <extension
         point="org.jcryptool.core.operations.analysis">
      <analysis
            contextHelpId="org.jcryptool.analysis.friedman.friedman"
            name="%extOperationsName"
            viewId="org.jcryptool.analysis.friedman.views.FriedmanView">
      </analysis>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="$nl$/contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="$nl$/toc.xml"
            primary="false">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.jcryptool.analysis.friedman.views.FriedmanView">
         <command
               commandId="org.jcryptool.analysis.friedman.helpCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/help_contents.gif"
               style="push"
               tooltip="%help.command.tooltip">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="menu:org.jcryptool.analysis.friedman.views.FriedmanView">
         <command
               commandId="org.jcryptool.analysis.friedman.helpCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/help_contents.gif"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.jcryptool.analysis.friedman.helpCommand"
            name="%help.command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.jcryptool.analysis.friedman.handler.HelpHandler"
            commandId="org.jcryptool.analysis.friedman.helpCommand">
      </handler>
   </extension>

</plugin>
